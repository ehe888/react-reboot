{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "glamorous-displayname",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
      }
    ],
    [
      "inline-import",
      {
        "extensions": [
          ".css",
          ".js"
        ]
      }
    ]
  ]
}